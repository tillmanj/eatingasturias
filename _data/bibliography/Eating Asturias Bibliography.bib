@book{Brillat-Savarin2019PhysiologyTasteMeditations,
  title = {The {{Physiology}} of {{Taste}} or {{Meditations}} on {{Transcendental Gastronomy}}},
  shorttitle = {Physiology of {{Taste}}},
  author = {{Brillat-Savarin}, Jean Anthelme},
  year = {2019},
