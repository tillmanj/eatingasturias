  keywords = {Cooking / General,Cooking / History,Cooking / Regional & Ethnic / European,Generally,History / Europe / General,PDF,Social Science / Anthropology / Cultural & Social,Social Science / Customs & Traditions,Social Science / Popular Culture}
}

