@article{Bowman2007DunaverneyLittleThetford,
  title = {The {{Dunaverney}} and {{Little Thetford Flesh-Hooks}}: {{History}}, {{Technology}} and {{Their Position}} within the {{Later Bronze Age Atlantic Zone Feasting Complex}}},
  shorttitle = {The {{Dunaverney}} and {{Little Thetford Flesh-Hooks}}},
  author = {Bowman, Sheridan and Needham, Stuart},
  year = {2007},
  month = sep,
  journal = {The Antiquaries Journal},
  volume = {87},
  pages = {53--108},
  issn = {0003-5815, 1758-5309},
  doi = {10.1017/S0003581500000846},
  url = {https://www.cambridge.org/core/product/identifier/S0003581500000846/type/journal_article},
  urldate = {2025-01-06},
  abstract = {Discovered in County Antrim and Cambridgeshire respectively, the Dunaverney and Little Thetford flesh-hooks are two of only thirty-six currently known examples from the Bronze Age of the Atlantic seaboard of Europe. Both are impressive and enigmatic objects and are among the most elaborate of later-series flesh-hooks dating to c 1100--800 BC. Not surprisingly, from the time it was found in 1829, Dunaverney was the subject of much antiquarian interest. Yet, despite their rarity and unusualness, the Dunaverney and Little Thetford flesh-hooks have never been adequately studied.             Our investigations have provided an understanding of the technology of these two fleshhooks, as well as new chronological information for the type as a whole. They have also revealed new uses of lost-wax casting in the British Isles, where the use of this technique is otherwise rare. The bird motifs on the Dunaverney flesh-hook remain unique, although it is now possible to set them against a broader background of iconographic representations on Atlantic feasting gear. Moreover, certain recurring design features may suggest that iconographic symbols were originally more often present on flesh-hooks. The findspot of Dunaverney lies at the heart of deposits of other contemporary prestige metalwork and that of Little Thetford within the greatest concentration of finds of the innovative Wilburton-stage metalworking tradition; both re-enforce the social significance of these rare objects.},
  copyright = {https://www.cambridge.org/core/terms},
  langid = {english},
  language = {en},
  keywords = {Generally,PDF}
}

